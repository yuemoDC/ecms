veIndex');\n      this.activeIndex = 'home';\n      this.$router.push('/OutScreen');\n    },\n    toggleTheme() {\n      this.isDark = !this.isDark;\n      localStorage.setItem('theme', this.isDark ? 'dark' : 'light');\n      this.applyTheme();\n    }\n  }\n};","map":{"version":3,"names":["gsap","name","data","activeIndex","isDark","created","localStorage","getItem","setItem","updateActiveIndex","applyTheme","watch","$route.path","methods","document","body","classList","toggle","addFloatingStars","removeStars","stars","createElement","className","appendChild","to","duration","backgroundPosition","repeat","ease","querySelector","remove","path","$route","indexMap","handleSelect","index","routeMap","$emit","handleLogout","removeItem","$router","push","toggleTheme"],"sources":["D:\\code_new\\ecms\\ecmsvue\\src\\components\\MerchantNavbar.vue"],"sourcesContent":["<template>\r\n  <el-header class=\"navbar-component\">\r\n    <div class=\"navbar\">\r\n      <el-menu\r\n          :default-active=\"activeIndex\"\r\n          class=\"menu\"\r\n          @select=\"handleSelect\"\r\n          mode=\"horizontal\"\r\n      >\r\n        <el-menu-item index=\"home\">È¶ñÈ°µ</el-menu-item>\r\n        <el-menu-item index=\"products\">‰∫ßÂìÅÁÆ°ÁêÜ</el-menu-item>\r\n        <el-menu-item index=\"orders\">ËÆ¢ÂçïÁÆ°ÁêÜ</el-menu-item>\r\n        <el-menu-item index=\"merchant-application\">ÂïÜÂÆ∂ÂÖ•È©ªÁî≥ËØ∑</el-menu-item>\r\n\r\n        <div class=\"logout-wrapper\">\r\n          <div class=\"theme-toggle\" @click=\"toggleTheme\">\r\n            {{ isDark ? '‚òÄÔ∏è Êó•Èó¥Ê®°Âºè' : 'üåô Â§úÈó¥Ê®°Âºè' }}\r\n          </div>\r\n          <el-menu-item index=\"logout\" @click=\"handleLogout\">ÁôªÂá∫</el-menu-item>\r\n        </div>\r\n      </el-menu>\r\n    </div>\r\n  </el-header>\r\n</template>\r\n\r\n<script>\r\nimport {gsap} from \"gsap\"\r\n\r\nexport default {\r\n  name: 'AppMerchantNavbar',\r\n  data() {\r\n    return {\r\n      activeIndex: 'home',\r\n      isDark: false\r\n    };\r\n  },\r\n  created() {\r\n    if (!localStorage.getItem('activeIndex')) {\r\n      localStorage.setItem('activeIndex', 'home');\r\n    }\r\n    this.updateActiveIndex();\r\n    this.applyTheme();\r\n  },\r\n  watch: {\r\n    '$route.path'() {\r\n      this.updateActiveIndex();\r\n    }\r\n  },\r\n  methods: {\r\n    applyTheme() {\r\n      document.body.classList.toggle('dark-mode', this.isDark);\r\n      if (this.isDark) {\r\n        this.addFloatingStars();\r\n      } else {\r\n        this.removeStars();\r\n      }\r\n    },\r\n    addFloatingStars() {\r\n      const stars = document.createElement('div');\r\n      stars.className = 'floating-stars';\r\n      document.body.appendChild(stars);\r\n\r\n      // ‰ΩøÁî®GSAPÂàõÂª∫ÊµÆÂä®Âä®Áîª\r\n      gsap.to('.floating-stars', {\r\n        duration: 50,\r\n        backgroundPosition: '-10000px 5000px',\r\n        repeat: -1,\r\n        ease: 'none'\r\n      });\r\n    },\r\n    removeStars() {\r\n      const stars = document.querySelector('.floating-stars');\r\n      if (stars) stars.remove();\r\n    },\r\n    updateActiveIndex() {\r\n      const path = this.$route.path;\r\n      const indexMap = {\r\n        '/home': 'home',\r\n        '/products': 'products',\r\n        '/orders': 'orders',\r\n        '/merchant-application': 'merchant-application'\r\n      };\r\n      this.activeIndex = indexMap[path] || '';\r\n      localStorage.setItem('activeIndex', this.activeIndex);\r\n    },\r\n    handleSelect(index) {\r\n      if (index === 'logout') return;\r\n\r\n      const routeMap = {\r\n        'home': '/home',\r\n        'products': '/products',\r\n        'orders': '/orders',\r\n        'merchant-application': '/merchant-application'\r\n      };\r\n\r\n      this.$emit('navigate', routeMap[index] || '/home');\r\n    },\r\n    handleLogout() {\r\n      localStorage.removeItem('token');\r\n      localStorage.removeItem('activeIndex');\r\n      this.activeIndex = 'home';\r\n      this.$router.push('/OutScreen');\r\n   