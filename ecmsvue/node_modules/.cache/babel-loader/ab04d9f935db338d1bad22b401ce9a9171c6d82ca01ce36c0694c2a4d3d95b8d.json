{"ast":null,"code":"import AppMerchantNavbar from '../components/MerchantNavbar.vue';\nimport AppAdminNavbar from '../components/AdminNavbar.vue'; // ÂºïÂÖ•ÁÆ°ÁêÜÂëòÂØºËà™Ê†è\nimport { gsap } from \"gsap\";\nexport default {\n  name: 'HomePage',\n  components: {\n    AppMerchantNavbar,\n    AppAdminNavbar\n  },\n  data() {\n    return {\n      currentUser: null,\n      currentTime: ''\n    };\n  },\n  mounted() {\n    this.initAnimations();\n  },\n  computed: {\n    navbarComponent() {\n      // Ê†πÊçÆÁî®Êà∑ËßíËâ≤ËøîÂõûÁõ∏Â∫îÁöÑÂØºËà™Ê†èÁªÑ‰ª∂\n      if (this.currentUser && this.currentUser.role === 'admin') {\n        return 'AppAdminNavbar'; // Â¶ÇÊûúÊòØÁÆ°ÁêÜÂëòÔºåËøîÂõûÁÆ°ÁêÜÂëòÂØºËà™Ê†è\n      }\n      return 'AppMerchantNavbar'; // ÈªòËÆ§ËøîÂõûÂïÜÂÆ∂ÂØºËà™Ê†è\n    }\n  },\n  created() {\n    this.getCurrentUser();\n    this.updateTime();\n    setInterval(this.updateTime, 1000);\n  },\n  methods: {\n    initAnimations() {\n      // Ê†áÈ¢òÂä®Áîª\n      gsap.fromTo(\".home-container h1\", {\n        opacity: 0,\n        y: -50\n      }, {\n        opacity: 1,\n        y: 0,\n        duration: 1,\n        ease: \"power4.out\"\n      });\n\n      // Ê¨¢Ëøé‰ø°ÊÅØÂä®Áîª\n      gsap.fromTo(\".welcome-msg\", {\n        opacity: 0,\n        x: -50\n      }, {\n        opacity: 1,\n        x: 0,\n        delay: 0.3,\n        duration: 0.8,\n        ease: \"back.out(1.7)\"\n      });\n\n      // ÂÖ¨ÂëäÊ†èÂä®Áîª\n      gsap.fromTo(\".notice-board\", {\n        scale: 0.8,\n        rotation: -3,\n        opacity: 0\n      }, {\n        scale: 1,\n        rotation: 0,\n        opacity: 1,\n        delay: 0.5,\n        duration: 0.2,\n        ease: \"elastic.out(1, 0.5)\"\n      });\n\n      // ÂàóË°®È°πÂä®Áîª\n      gsap.fromTo(\".notice-list li\", {\n        x: 100,\n        opacity: 0\n      }, {\n        x: 0,\n        opacity: 1,\n        stagger: 0.2,\n        delay: 0.8,\n        duration: 0.6,\n        ease: \"power2.out\"\n      });\n\n      // Áî®Êà∑Ê†áÁ≠æÊµÆÂä®Âä®Á