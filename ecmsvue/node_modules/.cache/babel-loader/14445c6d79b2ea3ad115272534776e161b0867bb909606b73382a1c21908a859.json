\r\n    return {\r\n      products: [],\r\n      isFormVisible: false,\r\n      selectedProduct: null,\r\n    };\r\n  },\r\n  methods: {\r\n    fetchProducts() {\r\n      fetch('http://localhost:8080/api/products') // Adjust URL to your backend\r\n          .then(response => response.json())\r\n          .then(data => {\r\n            this.products = data;\r\n          });\r\n    },\r\n    showForm() {\r\n      this.selectedProduct = null;\r\n      this.isFormVisible = true;\r\n    },\r\n    editProduct(product) {\r\n      this.selectedProduct = product;\r\n      this.isFormVisible = true;\r\n    },\r\n    deleteProduct(productId) {\r\n      fetch(`http://localhost:8080/api/products/${productId}`, {\r\n        method: 'DELETE'\r\n      }).then(() => {\r\n        this.fetchProducts();\r\n      });\r\n    }\r\n  },\r\n  mounted() {\r\n    this.fetchProducts();\r\n  }\r\n};\r\n</script>\r\n"],"mappings":"AAgBA,OAAOA,WAAU,MAAO,mBAAmB;AAE3C,eAAe;EACbC,UAAU,EAAE;IAAED;EAAY,CAAC;EAC3BE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,aAAa,EAAE,KAAK;MACpBC,eAAe,EAAE;IACnB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,aAAaA,CAAA,EAAG;MACdC,KAAK,CAAC,oCAAoC,EAAE;MAAA,CACvCC,IAAI,CAACC,QAAO,IAAKA,QAAQ,CAACC,IAAI,CAAC,CAAC,EAChCF,IAAI,CAACP,IAAG,IAAK;QACZ,IAAI,CAACC,QAAO,GAAID,IAAI;MACtB,CAAC,CAAC;IACR,CAAC;IACDU,QAAQA,CAAA,EAAG;MACT,IAAI,CAACP,eAAc,GAAI,IAAI;MAC3B,IAAI,CAACD,aAAY,GAAI,IAAI;IAC3B,CAAC;IACDS,WAAWA,CAACC,OAAO,EAAE;MACnB,IAAI,CAACT,eAAc,GAAIS,OAAO;MAC9B,IAAI,CAACV,aAAY,GAAI,IAAI;IAC3B,CAAC;IACDW,aAAaA,CAACC,SAAS,EAAE;MACvBR,KAAK,CAAC,sCAAsCQ,SAAS,EAAE,EAAE;QACvDC,MAAM,EAAE;MACV,CAAC,CAAC,CAACR,IAAI,CAAC,MAAM;QACZ,IAAI,CAACF,aAAa,CAAC,CAAC;MACtB,CAAC,CAAC;IACJ;EACF,CAAC;EACDW,OAAOA,CAAA,EAAG;IACR,IAAI,CAACX,aAAa,CAAC,CAAC;EACtB;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}